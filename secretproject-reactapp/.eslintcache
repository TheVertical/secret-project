izontal\r\n        }\r\n    }\r\n    //Функции React\r\n\r\n    //Собственные функции класса\r\n\r\n    render() {\r\n       //Проверяет ориентацию - выбирает нужный стиль для верстки\r\n        const checkHor = () => {\r\n            return (\r\n                this.state.IsHorizontal ? \"blockStyle\" : \"forColumnCase\"\r\n            )\r\n        }\r\n\r\n        return (\r\n        <div className={checkHor()}>\r\n            {/* <Navbar className=\"navbar-expand-sm navbar-toggleable-sm ng-white border-bottom box-shadow mb-3\" light>\r\n          <Container>\r\n            <NavbarBrand tag={Link} to=\"/\">WebApplication1</NavbarBrand>\r\n            <NavbarToggler onClick={this.toggleNavbar} className=\"mr-2\" />\r\n            <Collapse className=\"d-sm-inline-flex flex-sm-row-reverse\" isOpen={!this.state.collapsed} navbar>\r\n              <ul className=\"navbar-nav flex-grow\">\r\n                <NavItem>\r\n                  <NavLink tag={Link} className=\"text-dark\" to=\"/\">Home</NavLink>\r\n                </NavItem>\r\n                <NavItem>\r\n                  <NavLink tag={Link} className=\"text-dark\" to=\"/counter\">Counter</NavLink>\r\n                </NavItem>\r\n                <NavItem>\r\n                  <NavLink tag={Link} className=\"text-dark\" to=\"/fetch-data\">Fetch data</NavLink>\r\n                </NavItem>\r\n              </ul>\r\n            </Collapse>\r\n          </Container>\r\n        </Navbar> */}\r\n            {this.state.MainTitle}\r\n            {this.state.Links.map((link)=>\r\n             <NavLink className=\"stylishAAA\" key={link.Id} to={link.Link}>{link.Title}</NavLink>\r\n            )\r\n            }\r\n        </div>\r\n        \r\n        );\r\n    }\r\n\r\n}\r\n\r\nexport default LinksMenu;","C:\\Users\\Геннадий\\Desktop\\SecretProject\\SecretProject\\secretproject-reactapp\\src\\basedComponents\\ContactBlock.js",["189","190","191","192","193","194"],"//Библиотечные зависимости\r\nimport React from 'rea