{
    "compilerOptions": {
      "module": "es6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports":true,
      "noImplicitAny": true,
      "removeComments": true,
      "jsx": "react",
      "target": "es5",
      "lib": ["DOM", "ES2016"],
      "preserveConstEnums": true,
      "outDir": "./dist/",
      "sourceMap": true,
      "allowJs": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
  }